(set-logic ALL)
(set-option :produce-models true)
(set-option :incremental true)
(declare-const k Int)
(declare-const s (_ BitVec k))
(declare-const t (_ BitVec k))
(assert (distinct (bvshl (bvshl s (bvshl (_ bv1 k) (bvor s (_ bv1 k)))) t) (bvshl (bvshl s t) (bvshl (_ bv1 k) (bvand s (_ bv1 k))))))
