(set-logic ALL)
(set-option :produce-models true)
(set-option :incremental true)
(declare-const k Int)
(declare-const s (_ BitVec k))
(declare-const t (_ BitVec k))
(assert (distinct (bvlshr (bvshl (bvshl (_ bv1 k) t) (bvand s t)) (_ bv1 k)) (bvshl (bvshl (_ bv1 k) (bvshl (_ bv1 k) (bvand s t))) t)))
(check-sat)
(exit)