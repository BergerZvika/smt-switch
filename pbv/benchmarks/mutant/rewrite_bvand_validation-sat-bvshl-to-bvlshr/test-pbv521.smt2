(set-logic ALL)
(set-option :produce-models true)
(set-option :incremental true)
(declare-const k Int)
(declare-const s (_ BitVec k))
(declare-const t (_ BitVec k))
(assert (distinct (bvlshr (bvshl (bvshl (_ bv1 k) s) (bvand s t)) t) (bvshl (bvshl (bvshl (_ bv1 k) s) t) (bvand s t))))
(check-sat)
(exit)