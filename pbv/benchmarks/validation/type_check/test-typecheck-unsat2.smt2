(set-logic ALL)
(set-option :produce-models true)
(set-option :incremental true)
(declare-const k Int)
(declare-const m Int)
(declare-const r Int)
(declare-const x (_ BitVec k))
(declare-const y (_ BitVec k))
(declare-const z (_ BitVec m))
(declare-const w (_ BitVec r))
(assert (= (concat z (concat x y)) w))
(assert (= (concat w y) z))
(check-sat)
(exit)