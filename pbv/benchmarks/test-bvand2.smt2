(set-logic ALL)
(set-option :produce-models true)
(declare-const k Int)
(declare-const m Int)
(declare-const r Int)
(declare-const x (_ BitVec k))
(declare-const y (_ BitVec m))
(declare-const z (_ BitVec r))
