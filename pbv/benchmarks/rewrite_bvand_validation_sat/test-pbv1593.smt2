(set-logic ALL)
(set-option :produce-models true)
(set-option :incremental true)
(declare-const k Int)
(declare-const s (_ BitVec k))
(declare-const t (_ BitVec k))
(assert (not (distinct (bvlshr (bvadd (_ bv1 k) (bvand s t)) s) (bvlshr (_ bv1 k) (bvlshr s (bvand t (_ bv1 k)))))))
(check-sat)
(exit)