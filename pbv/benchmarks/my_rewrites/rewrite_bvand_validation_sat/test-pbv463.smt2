(set-logic ALL)
(set-option :produce-models true)
(set-option :incremental true)
(declare-const k Int)
(declare-const s (_ BitVec k))
(declare-const t (_ BitVec k))
(assert (not (distinct (bvshl (bvand (bvshl s s) (bvshl t (_ bv1 k))) (_ bv1 k)) (bvshl (bvshl (bvand s t) s) s))))
