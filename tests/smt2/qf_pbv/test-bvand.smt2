(set-logic ALL)
(set-option :produce-models true)
(declare-const k Int)
(declare-const m Int)
(declare-const r Int)
(declare-const x (_ BitVec k))
(declare-const y (_ BitVec m))
(declare-const z (_ BitVec r))
(assert (distinct x 0))
(assert (distinct y 0))
(assert (= z 0))
(assert (= z (bvand x y)))
(check-sat)
(exit)
